  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ZFPlayer
  pod 'SnapKit'
  pod 'ZFPlayer/Core', '~> 4.0'
  pod 'ZFPlayer/ControlView', '~> 4.0'
  pod 'ZFPlayer/AVPlayer', '~> 4.0'
#  pod 'ZFPlayer/ijkplayer', '~> 4.0'
  pod 'SJVideoPlayer'

  # Pods for M3U8Kit
  pod 'M3U8Kit'
end
